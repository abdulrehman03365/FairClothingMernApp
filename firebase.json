{
  "hosting": {
    "public": "client/build",
    "ignore": [
      "firebase.json",
   
      "**/.env/**",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }

,
      {

        "source": "/api/**",
        "function": "app"

      }  ,
    {
      "source":"/auth/**",
      "function":"app"
    }  ]
  }
}
